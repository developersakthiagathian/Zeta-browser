{
  "name": "Zeta",
  "version": "1.87.134",
  "description": "Next generation Zeta browser for macOS, Windows, Linux, and Android",
  "homepage": "https://github.com/Zeta/Zeta-browser#readme",
  "bugs": {
    "url": "https://github.com/Zeta/Zeta-browser/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zeta/Zeta-browser.git"
  },
  "license": "MPL-2.0",
  "author": {
    "name": "Zeta Software <support@Zeta.com>"
  },
  "scripts": {
    "apply_patches": "cd src/Zeta && npm run apply_patches --",
    "audit_deps": "cd src/Zeta && npm run audit_deps --",
    "build": "cd src/Zeta && npm run build --",
    "chromium_rebase_l10n": "cd src/Zeta && npm run chromium_rebase_l10n --",
    "cibuild": "cd src/Zeta && npm run cibuild --",
    "create_dist": "cd src/Zeta && npm run create_dist --",
    "delete_string_translations": "cd src/Zeta && npm run delete_string_translations --",
    "gclient": "cd src/Zeta && npm run gclient --",
    "gn_check": "cd src/Zeta && npm run gn_check --",
    "init": "node ./scripts/init.js",
    "lint": "cd src/Zeta && npm run lint --",
    "network-audit": "cd src/Zeta && npm run network-audit --",
    "pull_l10n": "cd src/Zeta && npm run pull_l10n --",
    "push_l10n": "cd src/Zeta && npm run push_l10n --",
    "start": "cd src/Zeta && npm run start --",
    "sync": "cd src/Zeta && npm run sync --",
    "test": "cd src/Zeta && npm run test --",
    "test-security": "cd src/Zeta && npm run test-security --",
    "test:scripts": "jest lib scripts --passWithNoTests",
    "update_patches": "cd src/Zeta && npm run update_patches --",
    "versions": "cd src/Zeta && npm run versions --"
  },
  "engines": {
    "node": ">=24.11.1 <25.0.0",
    "npm": ">=11.6.2"
  },
  "config": {
    "projects": {
      "Zeta-core": {
        "dir": "src/Zeta",
        "branch": "master",
        "repository": {
          "url": "https://github.com/Zeta/Zeta-core.git"
        }
      }
    }
  },
  "dependencies": {
    "chalk": "4.1.2"
  }
}
